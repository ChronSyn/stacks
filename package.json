{
  "name": "@mobily/stacks",
  "description": "âš¡ Build React Native views blazingly fast.",
  "version": "0.9.4",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
    "build": "tsc --outDir ./dist",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "types": "tsc -p ./tsconfig.json --noEmit --composite false",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "ci:check": "concurrently 'yarn test' 'yarn lint' 'yarn types' --names \"TEST,LINT,TYPES\" -c \"bgGreen.bold,bgMagenta.bold,bgBlue.bold\" --kill-others-on-fail",
    "generate:contributors": "yarn all-contributors generate",
    "generate:package": "bash ./scripts/package.sh"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "react-native",
    "react",
    "typescript",
    "stacks",
    "layout"
  ],
  "author": "mobily",
  "homepage": "https://mobily.github.io/stacks",
  "bugs": "https://github.com/mobily/stacks/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobily/stacks.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.59.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.36",
    "@types/react-native": "^0.62.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "all-contributors-cli": "^6.16.0",
    "babel-jest": "^26.0.1",
    "concurrently": "^5.2.0",
    "coveralls": "^3.1.0",
    "docusaurus": "^2.0.0-alpha.50",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "import-sort-style": "^6.0.0",
    "import-sort-style-mobily": "^0.1.0",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "^0.0.4",
    "react": "^16.13.1",
    "react-native": "^0.62.2",
    "react-native-testing-library": "^2.1.0",
    "react-test-renderer": "^16.13.1",
    "typescript": "^3.9.5"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "wonka": "^4.0.14"
  }
}
