{
  "name": "@mobily/stacks",
  "description": "âš¡ Build React Native views blazingly fast.",
  "version": "1.0.0",
  "source": "./src/Stacks.ts",
  "main": "./dist/stacks.js",
  "module": "./dist/stacks.mjs",
  "types": "./dist/types/src/Stacks.d.ts",
  "exports": {
    ".": {
      "import": "./dist/stacks.mjs",
      "require": "./dist/stacks.js",
      "types": "./dist/types/src/Stacks.d.ts",
      "source": "./src/Stacks.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "*.md",
    "index.js.flow",
    "bsconfig.json",
    "package.json"
  ],
  "scripts": {
    "build": "yarn rollup -c rollup.config.js && yarn generate:flow",
    "clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
    "test": "yarn jest --coverage",
    "re:clean": "yarn bsb -clean-world",
    "re:build": "yarn bsb -make-world",
    "re:dev": "yarn bsb -make-world -w",
    "generate:flow": "node ./scripts/generate-flow-files.js",
    "generate:contributors": "yarn all-contributors generate"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "react-native",
    "react",
    "typescript",
    "rescript",
    "reason",
    "flow",
    "stacks",
    "layout",
    "ui"
  ],
  "author": "Marcin Dziewulski <hello@mobily.pl>",
  "homepage": "https://mobily.github.io/stacks",
  "bugs": "https://github.com/mobily/stacks/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobily/stacks.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.59.0",
    "wonka": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.36",
    "@types/react-native": "^0.62.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "all-contributors-cli": "^6.16.0",
    "babel-jest": "^26.0.1",
    "bs-platform": "^8.2.0",
    "concurrently": "^5.2.0",
    "coveralls": "^3.1.0",
    "docusaurus": "^2.0.0-alpha.50",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "gentype": "^3.31.0",
    "import-sort-style": "^6.0.0",
    "import-sort-style-mobily": "^0.1.0",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "^0.0.4",
    "react": "^16.13.1",
    "react-native": "^0.62.2",
    "react-native-testing-library": "^2.1.0",
    "react-test-renderer": "^16.13.1",
    "reason-react": "^0.9.1",
    "reason-react-native": "^0.62.3",
    "typescript": "^3.9.5"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
